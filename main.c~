#include "libs.h"
#include "alg.h"

int main(int argc, char* argv[]) {
    if (!SDL_Init(SDL_INIT_VIDEO)) {
        SDL_Log("SDL_Init Error: %s", SDL_GetError());
        return 1;
    }
    SDL_FRect testRect = {30,40,50,50};
    SDL_Renderer *prenderer = NULL;
    
    
    SDL_Window* window = SDL_CreateWindow("SDL3 Test", 640, 480, 0);
    prenderer = SDL_CreateRenderer(window, "Main rand");
    if(prenderer == NULL){
      perror("Renderer creation error");
      exit(5);

    }
    if(!SDL_DrawRectangle(prenderer, &testRect)){
      fprintf(stderr, "Error rendering");
      exit(5);

    }
    
  


    SDL_Delay(2000);
    SDL_DestroyWindow(window);
    SDL_Quit();
    return 0;
}
